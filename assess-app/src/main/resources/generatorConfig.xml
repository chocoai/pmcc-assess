<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
    <classPathEntry
            location="D:\Java\tools\lib\mysql\mysql-connector-java\5.1.23\mysql-connector-java-5.1.23.jar"/>

    <context id="ticket-pms">

        <commentGenerator>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>

        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://192.168.2.206:3306/pmcc_chks"
                        userId="root" password="123456">
        </jdbcConnection>

        <javaTypeResolver>
            <property name="forceBigDecimals" value="true"/>
        </javaTypeResolver>

        <!-- entity 位置-->
        <javaModelGenerator targetPackage="com.copower.pmcc.assess.dal.entity"
                            targetProject="D:\IdeaProjects\pmcc-chks\chks-app\src\main\java">
            <property name="enableSubPackages" value="false"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <!-- mapper xml 位置 -->
        <sqlMapGenerator targetPackage="mybatis"
                         targetProject="D:\IdeaProjects\pmcc-chks\chks-app\src\main\resources/">
        </sqlMapGenerator>

        <!--mapper 接口位置-->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.copower.pmcc.assess.dal.mapper"
                             targetProject="D:\IdeaProjects\pmcc-chks\chks-app\src\main\java">
            <property name="enableSubPackages" value="false"/>
        </javaClientGenerator>

        <!-- 具体表 -->

        <table tableName="tb_chks_approval_business" domainObjectName="ChksApprovalBusiness">
            <generatedKey column="id" sqlStatement="MySql" identity="true"/>
        </table>
        <table tableName="tb_chks_approval_info" domainObjectName="ChksApprovalInfo">
            <generatedKey column="id" sqlStatement="MySql" identity="true"/>
        </table>
        <table tableName="tb_chks_approval_assess" domainObjectName="ChksApprovalAssess">
            <generatedKey column="id" sqlStatement="MySql" identity="true"/>
        </table>
        <table tableName="tb_chks_approval_assess_details" domainObjectName="ChksApprovalAssessDetails">
            <generatedKey column="id" sqlStatement="MySql" identity="true"/>
        </table>
    </context>

</generatorConfiguration>
