            </dependency>

            <dependency>
                <groupId>com.sun.mail</groupId>
                <artifactId>javax.mail</artifactId>
                <version>1.5.6</version>
            </dependency>

            <!-- https://mvnrepository.com/artifact/cn.songxinqiang/com.baidu.ueditor -->
